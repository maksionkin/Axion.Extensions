    <PackageTags>IFileProvider;Git;Git Smart SHH; GitSmartHttp; SmartSSH;File Provider;FileProvider</PackageTags>
    <RootNamespace>Axion.Extensions.FileProviders</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SSH.NET" Version="2025.0.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\Axion.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="README.nuget.md" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Axion.Extensions.FileProviders.Git.Smart\Axion.Extensions.FileProviders.Git.Smart.csproj" />
  </ItemGroup>
</Project>
