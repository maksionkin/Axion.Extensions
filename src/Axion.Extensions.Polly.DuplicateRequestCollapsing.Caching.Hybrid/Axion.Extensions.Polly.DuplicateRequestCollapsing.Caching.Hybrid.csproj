    </None>
    <None Include="README.nuget.md" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Axion.Extensions.Polly.Caching.Hybrid\Axion.Extensions.Polly.Caching.Hybrid.csproj" />
  </ItemGroup>
</Project>
